{
    "manifest_version": 2,
    "name": "Legacy Skin Wiki",
    "version": "0.1.0",
    "description": "__MSG_ext_desc__",
    "default_locale": "en",
    "author": "X_strumarium",
    "homepage_url": "https://github.com/X-strumarium/LegacySkinWiki.git",
    "icons": {
        "256": "icons/256.png",
        "96": "icons/096.png",
        "72": "icons/072.png",
        "64": "icons/064.png",
        "48": "icons/048.png",
        "32": "icons/032.png",
        "24": "icons/024.png",
        "16": "icons/016.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.wikipedia.org/w/*",
                "https://*.wikipedia.org/wiki/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_start"
        }
    ]
}